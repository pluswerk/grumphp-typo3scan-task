{
  "name": "pluswerk/grumphp-typo3scan-task",
  "description": "Scans code for usage of TYPO3 deprecated and or changed code.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Hofer",
      "email": "sebastian.hofer@pluswerk.ag",
      "homepage": "http://pluswerk.ag"
    }
  ],
  "autoload": {
    "psr-4": {
      "Pluswerk\\GrumPHPTypo3ScanTask\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pluswerk\\GrumPHPTypo3ScanTask\\Test\\": "test/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/pluswerk/scanner.git"
    }
  ],
  "require": {
    "php": ">=7.2",
    "phpro/grumphp": "0.19.*",
    "typo3/cms-scanner": "0.0.1",
    "typo3/cms-core": "*",
    "twig/twig": "^3.1"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5 || ^9.4"
  }
}
